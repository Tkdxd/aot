    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-validation": "^3.0.7",
    "reactflow": "^11.10.1",
    "reflect-metadata": "^0.1.13",
    "run-script-webpack-plugin": "^0.2.0",
    "rxjs": "^7.8.1",
    "sass": "^1.69.5",
    "slate": "^0.101.5",
    "slate-history": "^0.100.0",
    "slate-react": "^0.101.5",
