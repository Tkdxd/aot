{
  "name": "auto_of_things",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn build:next && yarn build:nest",
    "build:next": "next build",
    "build:nest": "nest build --path ./tsconfig.server.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/server/main.js",
    "start:next": "next dev",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch --path tsconfig.server.json",
    "start:debug": "nest start --path ./tsconfig.server.json --debug --watch",
    "start:docker": "docker run --rm -p 3000:3000 -p 7655:22 -it $(docker build --build-arg ssh_pub_key=\"$(cat ~/.ssh/id_rsa.pub)\" -q .)",
    "start:ssh": "ssh -o StrictHostKeyChecking=no -L 9221:localhost:9229 root@localhost -p 7655",
    "start:prod": "node dist/main",
    "start:proxy": "docker run --name nest-next-blog-proxy -v $(pwd)/nginx.conf:/etc/nginx/conf.d/default.conf:ro --add-host localnode:$(ifconfig en0 | grep inet | grep -v inet6 | awk '{print $2}') -p 8080:8080 -d nginx",
    "lint": "eslint \"{src,apps,libs,test}/**/*.tsx?\" --fix",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/platform-socket.io": "^10.3.1",
    "@nestjs/platform-ws": "^10.3.1",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/websockets": "^10.3.1",
    "@uiw/react-markdown-editor": "^6.0.0",
    "@uiw/react-textarea-code-editor": "1.4.4",
    "antd": "^5.12.1",
    "axios": "^1.6.2",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-remove-imports": "^1.7.1",
    "chart.js": "^4.4.1",
    "chrome-aws-lambda": "^10.1.0",
    "i": "^0.3.7",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.1",
    "moveable": "^0.52.0",
    "nest-next": "^10.1.0",
